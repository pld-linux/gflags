prefix=@prefix@
exec_prefix=${prefix}
libdir=@libdir@
includedir=${prefix}/include

Name: gflags
Version: 2.0
Description: A commandline flags library that allows for distributed flags
URL: http://code.google.com/p/gflags
Requires:
Libs: -L${libdir} -lgflags
Libs.private: -pthread -lpthread
Cflags: -I${includedir}
